# Copy this file to .env and fill in your values. Do NOT commit .env to git.

# Database connection - connects to the existing mage-postgres container
DATABASE_URL=postgresql+asyncpg://postgres:your-password@mage-postgres:5432/indian_sm_dw

# These should match your main indian_sm_dw project .env
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-password
POSTGRES_DB=indian_sm_dw
POSTGRES_DBNAME=indian_sm_dw
POSTGRES_HOST=mage-postgres
POSTGRES_PORT=5432

# Application settings
PORT=8000
DEBUG=false
